  "engines": {
    "node": "^10.17 || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "after": "ember-cli-babel",
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "10.21.0",
    "yarn": "1.22.4"
  }
